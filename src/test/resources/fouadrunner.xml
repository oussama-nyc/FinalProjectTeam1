<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="magento.softwaretestingboard.com test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="110"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://magento.softwaretestingboard.com/"></parameter>

    <test name="FouadTest ">
        <classes>

            <class name="com.luma.AccountTest"></class>
            <class name="com.luma.BaseMenuTest"></class>
            <class name="com.luma.CheckoutTest"></class>
            <class name="com.luma.RegisterTest"></class>
            <class name="com.luma.AccountTest"></class>
            <class name="com.luma.ShoppingCartTest"></class>

        </classes>
    </test>
</suite>